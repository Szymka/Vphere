<!-- components/timePicker/index.wxml -->
<picker class="max" disabled="{{disabled}}" mode="multiSelector" bindchange="pickerChange" bindcolumnchange="pickerColumnChange" bindcancel="pickerCancel" value="{{pickerIndex}}" range="{{pickerArray}}" range-key="{{'name'}}">
	<input placeholder="请选择时间" class="weui-input" disabled="{{true}}" value="{{dateString}}"></input>
</picker>